{
  "diagnostics": {
    "undefinedMethods": false,
    "undefinedFunctions": false,
    "undefinedVariable": true,
    "undefinedConstant": false,
    "undefinedProperty": false,
    "unusedUseDeclaration": false,
    "implementationErrors": false,
    "syntaxError": true,
    "parseError": true,
    "argumentCount": false,
    "typeError": false
  },
  "analysis": {
    "onSave": true,
    "typeAssertion": false,
    "nullableReturnType": false,
    "documentationKeywords": ["@deprecated"],
    "bare": false,
    "excludePaths": [
      "**/vendor/**"
    ]
  },
  "files": {
    "maxSize": 1000000,
    "exclude": [
      "**/.git",
      "**/node_modules",
      "**/vendor",
      "**/storage",
      "**/bootstrap/cache",
      "**/.env*"
    ]
  },
  "format": {
    "enable": false
  },
  "completion": {
    "maxItems": 100,
    "fullyQualifyGlobalConstantsAndFunctions": false,
    "insertUseDeclaration": false
  },
  "environment": {
    "includePaths": [],
    "stubs": [
      "bcmath",
      "bz2",
      "calendar",
      "com_dotnet",
      "core",
      "ctype",
      "curl",
      "date",
