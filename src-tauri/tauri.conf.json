{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "STEMS",
  "version": "0.1.0",
  "identifier": "com.stems.app",
  "build": {
    "frontendDist": "../public",
    "devUrl": "http://127.0.0.1:8000",
    "beforeDevCommand": null,
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "STEMS",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
